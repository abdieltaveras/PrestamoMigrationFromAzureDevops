@page "/localidadesnegocios"
@using PrestamoBlazorApp.Shared.Components.LocalidadesNegocios
@inherits BaseForCreateOrEdit
@using PrestamoBlazorApp.Shared.Components.Forms 
<GeneralList >
    <TableList>
        <MudTable Items="localidadesnegocios" Context="ct">
            <HeaderContent>
                    <MudTh>IdNegocio</MudTh>
                    <MudTh>Codigo</MudTh>
                    <MudTh>Nombre Comercial</MudTh>
                    <MudTh>Nombre Juridico</MudTh>
                    <MudTh>Accion</MudTh>
            </HeaderContent>
            <RowTemplate>
                <MudTd>@ct.IdNegocio</MudTd>
                <MudTd>@ct.Codigo</MudTd>
                <MudTd>@ct.NombreComercial</MudTd>
                <MudTd>@ct.NombreJuridico</MudTd>
                <MudTd>
                    <MudIconButton OnClick='()=>OnEditClick("localidadesnegocios/createoredit/" + ct.IdLocalidadNegocio)'
                                   Icon="@Icons.Filled.Edit">

                    </MudIconButton>
                </MudTd>
            </RowTemplate>
        </MudTable>
    </TableList>
</GeneralList>



