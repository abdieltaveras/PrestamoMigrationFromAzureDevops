<MudTextField Label="Nombre donde trabaja" @bind-Value="@infoLaboral.Nombre" Validation="@infoLaboral.Nombre"></MudTextField>

<MudTextField Label="Direccion" @bind-Value="@infoLaboral.Direccion" Validation="@infoLaboral.Direccion" Lines="2" ></MudTextField>

<MudTextField Label="Telefono 1" @bind-Value="@infoLaboral.NoTelefono1" Validation="@infoLaboral.NoTelefono1"></MudTextField>

<MudTextField Label="Telefono 2" @bind-Value="@infoLaboral.NoTelefono2" Validation="@infoLaboral.NoTelefono2"></MudTextField>
        
<MudTextField Label="Puesto o funcion" @bind-Value="@infoLaboral.Puesto" Validation="@infoLaboral.Puesto"></MudTextField>

<MudTextField Label="Otros detalles" @bind-Value="@infoLaboral.Notas" Validation="@infoLaboral.Notas" Lines="3"></MudTextField>

@code {
    [Parameter]
    public InfoLaboral infoLaboral { get; set; } = new InfoLaboral();
}