<MudDialog>

    <DialogContent>
        @Content
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="OnSaveClick">Guardar</MudButton>
    </DialogActions>
</MudDialog>
@code {

    [Parameter]
    public RenderFragment? Content { get; set; }
    [Parameter]
    public EventCallback<MouseEventArgs> OnSaveClick { get; set; }
}
