<!DOCTYPE html>
<html lang="es-do">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title | PCP Prestamos</title>
    <link href="~/Content/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/content/vendors/iCheck/skins/flat/green.css" rel="stylesheet">
    <link href="~/Content/vendors/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css" rel="stylesheet" />
    <!--For abdiel layout-->
    @{
        if (@ViewBag.LoadCssAndJsGrp2 != null && @ViewBag.LoadCssAndJsGrp2)
        {
            <!--pnotify section-->
            <link href="~/Content/vendors/pnotify/dist/pnotify.css" rel="stylesheet" />
            <link href="~/Content/vendors/pnotify/dist/pnotify.buttons.css" rel="stylesheet" />
            <link href="~/Content/vendors/pnotify/dist/pnotify.nonblock.css" rel="stylesheet" />

            <link href="~/content/vendors/bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.css" rel="stylesheet">
            <link href="~/content/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
            <link href="~/Content/themes/base/jquery-ui.css" rel="stylesheet" />
            <link href="~/content/vendors/select2/dist/css/select2.min.css" rel="stylesheet">
            <!--For abdiel layout-->
            <!-- scriptst neccesary at head  if it is placed down some pages will not work correctly-->
        }
        // Definir la condicion para cargar las librerias del datatable
        if (@ViewBag.LoadCssAndJsForDatatable != null && @ViewBag.LoadCssAndJsForDatatable)
        {
            <link rel="stylesheet" href="~/Content/customcss/datatable/css/dataTables.bootstrap4.min.css">
            <link rel="stylesheet" href="~/Content/customcss/datatable/css/buttons.dataTables.min.css">

            <link href="~/Content/customcss/datatable/css/customStyle.css" rel="stylesheet">

        }
    }
    <link href="~/content/vendors/switchery/dist/switchery.min.css" rel="stylesheet">
    @*<link href="~/content/vendors/switchery/switchery.js" rel="stylesheet">*@
    <link href="~/Content/build/css/custom.css" rel="stylesheet" />
    <link href="~/Content/customcss/global-styles.css" rel="stylesheet" />
    <script src="~/Content/vendors/jquery/dist/jquery.min.js"></script>
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
    @RenderSection("pagestyle", required: false)
</head>

<body class="nav-md">
    @RenderBody()

    @RenderSection("prevscripts", required: false)
    <script src="~/Content/vendors/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <script src="~/Content/vendors/Chart.js/dist/Chart.min.js"></script>
    <script src="~/Content/vendors/DateJS/build/date.js"></script>
    <script src="~/Content/vendors/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"></script>
    @{
        if (@ViewBag.LoadCssAndJsGrp2 != null && @ViewBag.LoadCssAndJsGrp2)
        {
            <!--abdiel section-->
            <script src="~/content/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>
            <script src="~/content/vendors/devbridge-autocomplete/dist/jquery.autocomplete.min.js"></script>
            <script src="~/content/vendors/jquery.inputmask/dist/min/jquery.inputmask.bundle.min.js"></script>
            <script src="~/Scripts/jquery.validate.min.js"></script>
            <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
            <script src="~/Scripts/Apps/minMaxAdapter.js"></script>
            <script src="~/Scripts/jquery-ui-1.12.1.js"></script>
            <script src="~/content/vendors/moment/min/moment.min.js"></script>
            <!--abdiel section-->
        }
        // Definir la condicion para cargar las librerias del datatable
        if (@ViewBag.LoadCssAndJsForDatatable != null && @ViewBag.LoadCssAndJsForDatatable)
        {
            <!--LoadCssAndJsForDatatable section-->
            <script src="~/Content/customcss/datatable/js/jquery.dataTables.min.js"></script>
            <script src="~/Content/customcss/datatable/js/dataTables.bootstrap4.min.js"></script>
            <script src="~/Content/customcss/datatable/js/dataTables.buttons.min.js"></script>
            <script src="~/Content/customcss/datatable/js/buttons.html5.min.js"></script>

            <script src="~/Content/customcss/datatable/js/jszip.min.js"></script>
            <script src="~/Content/customcss/datatable/js/buttons.print.min.js"></script>
            <script src="~/Content/customcss/datatable/js/pdfmake.min.js"></script>
            <script src="~/Content/customcss/datatable/js/vfs_fonts.js"></script>
            <script src="~/Content/customcss/datatable/configuration/js-conf/datatable-conf.js"></script>

            // for multi select
            @*<script type="text/javascript" src="//gyrocode.github.io/jquery-datatables-checkboxes/1.2.11/js/dataTables.checkboxes.min.js"></script>
                <link type="text/css" href="//gyrocode.github.io/jquery-datatables-checkboxes/1.2.11/css/dataTables.checkboxes.css" rel="stylesheet" />*@
            <script type="text/javascript" src="~/Content/vendors/datatables-checkbox/dataTables.checkboxes.min.js"></script>
            <link type="text/css" href="~/Content/vendors/datatables-checkbox/dataTables.checkboxes.css" rel="stylesheet" />

            //for single select
            @*<script type="text/javascript" src="https://cdn.datatables.net/select/1.3.1/js/dataTables.select.min.js"></script>
                <link type="text/css" href="https://cdn.datatables.net/select/1.3.1/css/select.dataTables.min.css" rel="stylesheet" />*@
            <script type="text/javascript" src="~/Content/vendors/datatables-select/dataTables.select.min.js"></script>
            <link type="text/css" href="~/Content/vendors/datatables-select/select.dataTables.min.css" rel="stylesheet" />
            <!--end LoadCssAndJsForDatatable section-->
        }

    }
    <script src="~/content/vendors/iCheck/icheck.min.js"></script>
    <script src="~/content/vendors/parsleyjs/dist/parsley.min.js"></script>
    <script src="~/content/vendors/google-code-prettify/src/prettify.js"></script>
    <script src="~/content/vendors/select2/dist/js/select2.full.min.js"></script>
    <script src="~/content/vendors/switchery/dist/switchery.min.js"></script>
    <script src="~/Content/vendors/bootstrap-wysiwyg/js/bootstrap-wysiwyg.min.js"></script>
    <script src="~/Content/build/js/custom.js"></script>
    <!--pnotify section-->
    <script src="~/Content/vendors/pnotify/dist/pnotify.js"></script>
    <script src="~/Content/vendors/pnotify/dist/pnotify.buttons.js"></script>
    <script src="/Content/vendors/pnotify/dist/pnotify.nonblock.js"></script>
    <script src="~/Scripts/Apps/Utils.js"></script>
    <script src="~/Scripts/Apps/UtilsForJs.js"></script>
    @RenderSection("scripts", required: false)
</body>

