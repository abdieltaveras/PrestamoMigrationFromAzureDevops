                        <div class="profile clearfix ">
                            <div class="profile_pic">
                                @*<img src="@AuthInSession.GetUserImageFilePath()" alt="..." class="img-circle profile_img">*@
                                <img class="img-circle profile_img" src="@Url.Content(SiteImages.AppLogoSinLetra)" />
                            </div>
                            <div class="profile_info">
                                <div class="row">
                                    <span>PCP PRESTAMO</span>
                                </div>
                                <div class="row">
                                    <a href="@Url.Action("Index", "Home")"><i class="fa fa-home"></i> Inicio </a>
